{
    "id": "ecspresso",
    "version": "0.0.1",
    "name": "ecspresso",
    "documentationURL": "https://github.com/rhiroe/features/src/ecspresso",
    "description": "Provides ecspresso.",
    "keywords": ["ecspresso"],
    "options": {
        "ecspressoVersion": {
            "type": "string",
            "default": "latest",
            "proposals": [
                "latest",
                "2.4.1"
            ],
            "description": "Version of ecspresso to install."
        }
    },
    "mounts": [
        {
            "source": "${localEnv:HOME}/.aws/credentials",
            "target": "/root/.aws/credentials",
            "type": "bind"
        }
    ],
    "installsAfter": [
        "ghcr.io/devcontainers/features/aws-cli"
    ]
}

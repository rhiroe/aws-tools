{
    "id": "saml2aws",
    "version": "0.1.0",
    "name": "saml2aws",
    "documentationURL": "https://github.com/rhiroe/features/src/saml2aws",
    "description": "Provides saml2aws.",
    "keywords": ["saml2aws"],
    "options": {
        "saml2awsVersion": {
            "type": "string",
            "default": "latest",
            "proposals": [
                "latest",
                "2.36.17"
            ],
            "description": "Version of saml2aws to install."
        }
    },
    "mounts": [
        {
            "source": "${localEnv:HOME}/.aws/config",
            "target": "/root/.aws/config",
            "type": "bind"
        },
        {
            "source": "${localEnv:HOME}/.saml2aws",
            "target": "/root/.saml2aws",
            "type": "bind"
        }
    ],
    "installsAfter": [
        "ghcr.io/devcontainers/features/aws-cli"
    ]
}

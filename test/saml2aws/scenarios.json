{
    "with_root_user": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "remoteUser": "root",
        "features": {
            "saml2aws": {}
        }
    },
    "with_non_root_user": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
